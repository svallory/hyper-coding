{
  "$schema": "https://www.mintlify.com/docs.json",
  "name": "HyperDev",
  "theme": "aspen",
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "icons": {
    "library": "lucide"
  },
  "logo": {
    "dark": "/logo/logo.png",
    "light": "/logo/logo.png"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#252525",
    "light": "#d4a280",
    "dark": "#252525"
  },
  "background": {
    "color": {
      "light": "#fdfdf7",
      "dark": "#0b0a0b"
    }
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Github",
          "icon": "github",
          "href": "https://github.com/subtletools/hyperdev"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "get-started/installation",
              "get-started/quickstart",
              "get-started/tutorial"
            ]
          },
          {
            "group": "What is Hyper Coding?",
            "pages": [
              "hyper-coding/introduction-to-hyper-coding",
              "hyper-coding/hyper-vs-vibe"
            ]
          },
          {
            "group": "How To",
            "pages": [
              "workflow/workflow-overview",
              "workflow/gen",
              "workflow/plan",
              "workflow/dev"
            ]
          },
          {
            "group": "Advanced Topics",
            "pages": [
              "advanced/customize-setup"
            ]
          },
          {
            "group": "CLI",
            "pages": [
              "cli/overview",
              "cli/quickstart",
              {
                "group": "Core Commands",
                "icon": "terminal",
                "pages": [
                  "cli/commands/overview",
                  {
                    "group": "Workflow Commands",
                    "pages": [
                      "cli/commands/workflow/init",
                      "cli/commands/workflow/gen",
                      "cli/commands/workflow/plan",
                      "cli/commands/workflow/dev"
                    ]
                  },
                  {
                    "group": "Discovery Commands",
                    "pages": [
                      "cli/commands/discovery/search",
                      "cli/commands/discovery/list"
                    ]
                  },
                  {
                    "group": "Resource Commands",
                    "pages": [
                      "cli/commands/resources/docs",
                      "cli/commands/resources/plan",
                      "cli/commands/resources/task"
                    ]
                  },
                  {
                    "group": "Dashboard",
                    "pages": [
                      "cli/commands/dash"
                    ]
                  }
                ]
              }
            ]
          },
          {
            "group": "Tools & Integrations",
            "pages": [
              {
                "group": "AI Tools",
                "icon": "bot",
                "pages": [
                  "ai-tools/claude-code",
                  "ai-tools/cursor",
                  "ai-tools/windsurf"
                ]
              },
              {
                "group": "Tool Integrations",
                "icon": "puzzle",
                "pages": [
                  "integrations/vscode",
                  "integrations/jetbrains",
                  "integrations/vim",
                  "integrations/vite",
                  "integrations/webpack",
                  "integrations/nextjs",
                  "integrations/github-actions",
                  "integrations/docker",
                  "integrations/git-hooks",
                  "integrations/claude-code-hooks",
                  "integrations/user-analytics-scenario",
                  "integrations/package-scripts"
                ]
              }
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/overview",
              "advanced-usage",
              "guides/best-practices",
              "security-guide",
              "development"
            ]
          },
          {
            "group": "Templates",
            "pages": [
              "templates/creating-templates",
              "templates/sharing-templates",
              "templates/migration-guide"
            ]
          },
          {
            "group": "Examples",
            "pages": [
              "examples/overview",
              "examples/quick-wins",
              "examples/real-world",
              "examples/advanced-patterns",
              "examples/frameworks"
            ]
          },
          {
            "group": "Community",
            "icon": "users",
            "pages": [
              "community/overview",
              "community-guidelines",
              "plugin-development"
            ]
          }
        ]
      },
      {
        "tab": "References",
        "groups": [
          {
            "pages": [
              {
                "icon": "terminal",
                "root": "CLI Reference"
              },
              {
                "icon": "cog",
                "root": "Configuration Guide"
              },
              {
                "icon": "bug",
                "root": "Troubleshooting"
              }
            ]
          },
          {
            "group": "mintlify-tsdocs",
            "icon": "code",
            "pages": [
              "packages/mintlify-tsdocs/index",
              {
                "group": "Classes",
                "icon": "cube",
                "pages": [
                  "packages/mintlify-tsdocs/mintlify-tsdocs.custommarkdownemitter",
                  "packages/mintlify-tsdocs/mintlify-tsdocs.markdowndocumenter"
                ]
              },
              {
                "group": "Interfaces",
                "icon": "square-dashed",
                "pages": [
                  "packages/mintlify-tsdocs/mintlify-tsdocs.icustommarkdownemitteroptions",
                  "packages/mintlify-tsdocs/mintlify-tsdocs.imarkdowndocumenteroptions",
                  "packages/mintlify-tsdocs/mintlify-tsdocs.imarkdownemittercontext",
                  "packages/mintlify-tsdocs/mintlify-tsdocs.imarkdownemitteroptions"
                ]
              }
            ]
          }
        ]
      }
    ]
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "search": {
    "prompt": "Search HyperDev docs..."
  },
  "modeToggle": {
    "default": "light",
    "isHidden": false
  },
  "topbarLinks": [
  ],
  "topbarCtaButton": {
    "name": "GitHub",
    "url": "https://github.com/subtletools/hyperdev",
    "style": "roundedFull"
  },
  "seo": {
    "indexHiddenPages": false
  },
  "redirects": [
    {
      "source": "/docs",
      "destination": "/"
    },
    {
      "source": "/getting-started",
      "destination": "/quickstart"
    }
  ],
  "openGraph": {
    "enabled": true,
    "logo": "/logo/logo.png"
  },
  "tabs": [
  ],
  "footerSocials": {
    "github": "https://github.com/subtletools/hyperdev"
  }
}