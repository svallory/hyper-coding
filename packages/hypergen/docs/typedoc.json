{
  "entryPoints": ["../../../packages/hypergen/src/index.ts"],
  "tsconfig": "../../../packages/hypergen/tsconfig.json",
  "out": "../../../apps/docs/api-reference/generated",
  "plugin": ["typedoc-plugin-markdown", "typedoc-theme-mintlify"],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "categorizeByGroup": true,
  "categoryOrder": ["Core", "Engine", "Template System", "Recipe System", "Actions", "Configuration", "Utilities", "*"],
  "readme": "none",
  "outputFileStrategy": "members",
  "fileExtension": ".mdx",
  "publicPath": "/api-reference/generated/",
  "githubPages": false,
  "hideGenerator": true,
  "skipErrorChecking": true,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Method",
    "Function",
    "Accessor",
    "Variable"
  ],
  "markdownItOptions": {
    "anchorPrefix": "api-"
  },
  "theme": "markdown",
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/hyperdev-official/hypergen/blob/{gitRevision}/{path}#L{line}",

  "mintlifyNavigation": "folder,kind",
  "mintlifySidebarIcons": "groups",
  "mintlifyDocsJsonPath": "../../../apps/docs/docs.json",
  "mintlifyTabName": "Hypergen SDK (TypeDoc MDX)"
}