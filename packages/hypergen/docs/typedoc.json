{
  "entryPoints": ["../src/index.ts"],
  "out": "../../../apps/docs/api-reference/generated",
  "plugin": ["typedoc-plugin-markdown"],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": true,
  "categorizeByGroup": true,
  "categoryOrder": ["Core", "Engine", "Template System", "Recipe System", "Actions", "Configuration", "Utilities", "*"],
  "readme": "none",
  "outputFileStrategy": "members",
  "fileExtension": ".mdx",
  "publicPath": "/api-reference/generated/",
  "githubPages": false,
  "hideGenerator": true,
  "skipErrorChecking": true,
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module", 
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "Type alias",
    "Constructor",
    "Property",
    "Method",
    "Function",
    "Accessor",
    "Variable"
  ],
  "markdownPlugins": {
    "anchorPrefix": "api-",
    "frontmatter": {
      "title": "{{ name }}",
      "description": "{{ description }}",
      "sidebar_position": "{{ index }}"
    }
  },
  "theme": "markdown",
  "disableSources": false,
  "sourceLinkTemplate": "https://github.com/hyperdev-official/hypergen/blob/{gitRevision}/{path}#L{line}"
}