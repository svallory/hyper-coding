{
	"name": "@hypercli/autocomplete",
	"version": "0.1.0",
	"type": "module",
	"description": "Autocomplete plugin for HyperCLI with dynamic kit/cookbook/recipe completion",
	"author": "Saulo Vallory <saulo@toki.life>",
	"license": "MIT",
	"engines": { "node": ">=20" },
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": ["dist", "oclif.manifest.json"],
	"oclif": {
		"commands": "./dist/commands",
		"topicSeparator": " "
	},
	"imports": {
		"#*": "./dist/*.js"
	},
	"scripts": {
		"build": "tsc",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "biome check .",
		"format": "biome format . --write",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@hypercli/core": "workspace:*",
		"@hypercli/ui": "workspace:*",
		"@oclif/core": "^4",
		"debug": "^4.4.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/debug": "^4.1.12",
		"@types/node": "^20.0.0",
		"typescript": "^5.4.0",
		"vitest": "^1.0.0"
	}
}
