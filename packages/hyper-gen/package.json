{
  "name": "@hypercli/gen",
  "version": "0.0.0",
  "type": "module",
  "description": "Code generation engine for HyperDev",
  "keywords": ["hyperdev", "hypergen", "code-generator"],
  "author": "Saulo Vallory <saulo@toki.life>",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "oclif.manifest.json",
    "README.md",
    "LICENSE"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "hooks": {
      "command_not_found": "./dist/hooks/command-not-found"
    },
    "topicSeparator": " "
  },
  "imports": {
    "#/*": "./src/*",
    "#/tests/*": "./tests/*",
    "#/fixtures/*": "./tests/fixtures/*",
    "#/helpers/*": "./tests/helpers/*"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "format": "biome format . --write",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "@hypercli/core": "workspace:*",
    "@hypercli/kits": "workspace:*",
    "@clack/prompts": "^0.11.0",
    "@jig-lang/jig": "^1.0.1",
    "@oclif/core": "^4",
    "ai": "^4.0.0",
    "chalk": "^4.1.2",
    "debug": "^4.4.3",
    "execa": "^5.1.1",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.1.0",
    "glob": "^11.0.3",
    "js-yaml": "^4.1.0",
    "typescript": "^5.4.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/fs-extra": "^11.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0"
  }
}
