{
	"files": {
		"ignore": ["dist", "node_modules", ".moon"]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"suspicious": {
				"noExplicitAny": "off",
				"noImplicitAnyLet": "off",
				"noUnsafeDeclarationMerging": "off"
			},
			"complexity": {
				"noBannedTypes": "off",
				"noStaticOnlyClass": "off",
				"noForEach": "off"
			},
			"style": {
				"noNonNullAssertion": "off",
				"noParameterAssign": "off"
			}
		}
	}
}
