{
  "name": "@hypercli/core",
  "version": "0.0.0",
  "type": "module",
  "description": "Core domain types, config, errors for HyperDev",
  "keywords": [
    "hyperdev",
    "hypergen",
    "code-generator"
  ],
  "author": "Saulo Vallory <saulo@toki.life>",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "imports": {
    "#/*": "./src/*",
    "#tests/*": "./tests/*",
    "#fixtures/*": "./tests/fixtures/*",
    "#helpers/*": "./tests/helpers/*"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "format": "biome format . --write",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "inflection": "^1.13.4",
    "change-case": "^3.1.0",
    "cosmiconfig": "^9.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/inflection": "^2.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.0.0"
  }
}
