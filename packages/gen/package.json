{
	"name": "@hypercli/gen",
	"version": "0.1.0",
	"type": "module",
	"description": "Code generation engine for HyperDev",
	"keywords": ["hyperdev", "hypergen", "code-generator"],
	"author": "Saulo Vallory <saulo@toki.life>",
	"license": "MIT",
	"engines": {
		"node": ">=20"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": ["dist", "help", "oclif.manifest.json", "README.md", "LICENSE"],
	"oclif": {
		"commands": "./dist/commands",
		"hooks": {
			"command_not_found": "./dist/hooks/command-not-found.js"
		},
		"topicSeparator": " "
	},
	"imports": {
		"#discovery": "./dist/discovery/index.js",
		"#discovery/*": "./dist/discovery/*.js",
		"#utils/*": "./dist/utils/*.js",
		"#ai": "./dist/ai/index.js",
		"#ai/*": "./dist/ai/*.js",
		"#hooks/*": "./dist/hooks/*.js",
		"#actions": "./dist/actions/index.js",
		"#actions/*": "./dist/actions/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#prompts/*": "./dist/prompts/*.js",
		"#ops/*": "./dist/ops/*.js",
		"#commands/*": "./dist/commands/*.js",
		"#recipe-engine": "./dist/recipe-engine/index.js",
		"#recipe-engine/*": "./dist/recipe-engine/*.js",
		"#template-engines": "./dist/template-engines/index.js",
		"#template-engines/*": "./dist/template-engines/*.js",
		"#tests/*": "./tests/*.ts",
		"#fixtures/*": "./tests/fixtures/*.ts",
		"#*": "./dist/*.js"
	},
	"scripts": {
		"build": "tsc && bun run copy-assets",
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"copy-assets": "mkdir -p dist/ai && cp src/ai/prompt-template.jig dist/ai/",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "biome check .",
		"format": "biome format . --write",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "bun run build"
	},
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"@hypercli/core": "workspace:*",
		"@hypercli/kit": "workspace:*",
		"@hypercli/ui": "workspace:*",
		"@jig-lang/jig": "^1.0.1",
		"@oclif/core": "^4",
		"ai": "^4.0.0",
		"chalk": "^4.1.2",
		"debug": "^4.4.3",
		"disparity": "^3.2.0",
		"execa": "^5.1.1",
		"fastest-levenshtein": "^1.0.16",
		"front-matter": "^4.0.2",
		"fs-extra": "^10.1.0",
		"glob": "^11.0.3",
		"js-yaml": "^4.1.0",
		"typescript": "^5.4.0",
		"zod": "^3.22.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/fs-extra": "^11.0.0",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^20.0.0",
		"typescript": "^5.4.0",
		"vitest": "4.0.18"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
