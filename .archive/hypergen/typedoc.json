{
	"$schema": "https://typedoc.org/schema.json",
	"name": "Hypergen API Documentation",
	"entryPoints": ["./src/index.ts"],
	"out": "../../apps/docs/packages/hypergen-typedoc",
	"plugin": [],
	"readme": "README.md",
	"excludePrivate": true,
	"excludeProtected": true,
	"excludeInternal": true,
	"excludeExternals": true,
	"exclude": [
		"**/node_modules/**",
		"**/dist/**",
		"**/tests/**",
		"**/*.spec.ts",
		"**/*.test.ts",
		"**/temp/**",
		"**/examples/**",
		"**/fixtures/**"
	],
	"includeVersion": true,
	"sort": ["source-order"],
	"categorizeByGroup": false,
	"defaultCategory": "Other",
	"categoryOrder": [
		"Core",
		"Configuration",
		"Templates",
		"Actions",
		"CLI",
		"Utilities",
		"Types",
		"*"
	],
	"validation": {
		"notExported": true,
		"invalidLink": true,
		"notDocumented": false
	},
	"treatWarningsAsErrors": false,
	"treatValidationWarningsAsErrors": false,
	"logLevel": "Info"
}
